{
  "expo": {
    "name": "awave",
    "slug": "awave",
    "version": "0.0.12",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.killuhwhale.awave",
      "icon": "./assets/icon.png",
      "splash": {
        "image": "./assets/splash.png"
      },
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSExceptionDomains": {}
        },
        "NSCameraUsageDescription": "Allow microphone features?",
        "NSMicrophoneUsageDescription": "Allow microphone features?"
      }
    },
    "android": {
      // "versionCode": 3, NEED TO UPDATE!!!!!!!!!!
      "versionCode": 3,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.killuhwhale.awave"
    },
    "platforms": ["ios", "android"],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5dda1433-9d6f-410f-a6a6-57a481836de1"
      }
    }
  }
}


